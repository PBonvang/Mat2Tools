<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mat2Tools := module()
description &quot;Procedures and guides associated with the Advanced Engineering Mathematics 2 (DTU 01037)&quot;;
option package;
export routhHurwitzMatrices, FourierCoefficients, FourierSeries, periodicFunc, convergenceRadius, dtransform;

local routhHurwitzMatrix;

routhHurwitzMatrix := proc(coefficients := {list})
    local Dl,s,i,j, d, c;
    c := coefficients;
    d := numelems(c)-1;
    if(c[1] &lt; 0) then
        c := -c;

    end if;

    Dl := [];
    for i from 1 by 1 to d-1 do
        s := [seq(0,1..floor((i-1)/2))];
        if (type(i,even)) then
            s := [op(s),1];

        end if;

        Dl := [op(Dl),op(s)];
        s := [0,op(s)];

        for j from s[numelems(s)]+1 by 2 to d do
            if(j+1 &gt; numelems(c)) then
                Dl := [op(Dl), 0];

            else
                Dl := [op(Dl), c[j+1]]

            end if;
        end do;
    end do;

    return Matrix(d-1,d-1,Dl);
end proc:

routhHurwitzMatrices := proc(A::{matrix, Matrix})
    local c, rd, cd, p, i;
    rd, cd := LinearAlgebra[Dimension](A);

    if(rd &lt;&gt; cd) then
        return &quot;Matrix must be square&quot;;

    elif (rd &lt;= 2) then
        return &quot;&quot;;

    end if;
    
    p := LinearAlgebra[CharacteristicPolynomial](A, lambda);
    c := coeffs(p, lambda);

    if(numelems([c]) &lt;= rd) then
        return &quot;One or more coefficient is zero&quot;;
    end if;
    
    for i from 1 by 1 to rd-2 do 
        routhHurwitzMatrix([c[i..numelems([c])]]);
    end do;

end proc:

FourierCoefficients := proc(f::{procedure}, {T := 2*Pi, odd::{boolean} := false, even::{boolean} := false, integralInterval::{range} := 0..T})
local a, b, c, var, w, i;
var := op(1, eval(f));
w := 2*Pi/T;

if (numelems([var]) &lt;&gt; 1) then
    return &quot;f can only have one independent variable.&quot;
end if;

if (add(seq(abs(op(integralInterval)[i]), i=1..2)) &lt;&gt; T) then
    print(&quot;The integral interval has to have the same length as [0;T]&quot;);
    return;
end if;

if (even) then
a := n -&gt; 4/T * int(f(var)*cos(n*w*var),var=0..T/2);
b := n -&gt; 0;

elif(odd) then
a := n -&gt; 0;
b := n -&gt; 4/T * int(f(var)*sin(n*w*var),var=0..T/2);

else
a := n -&gt; 2/T * int(f(var)*cos(n*w*var),x=integralInterval);
b := n -&gt; 2/T * int(f(var)*sin(n*w*var),x=integralInterval);

end if;
c := n -&gt; 1/T * int(f(var)*exp(-I*n*w*var),var=integralInterval);

return a, b, c;
end proc:

FourierSeries := proc(f::{procedure}, {T := 2*Pi, odd::{boolean} := false, even::{boolean} := false, integralInterval::{range} := 0..T, complex::{boolean} := false})
local a, b, c, var, w;
var := op(1, eval(f));
w := 2*Pi/T;

a, b, c := FourierCoefficients(f, T=T,odd=odd,even=even,integralInterval=integralInterval,complex=complex);

if(complex) then
    return unapply(sum(c(n)*exp(i*n*w*var),n=-infinity..infinity),var);
end if;

return unapply(1/2*a(0)+sum(a(n)*cos(n*w*var) + b(n)*sin(n*w*var),n=1..infinity),var);

end proc:

periodicFunc := proc(f::{procedure}, {range::range := -Pi..Pi})
local l,h, period;

l,h := op(convert(range,list));
period := h-l;

return unapply(f(x - floor((x-l)/period)*period),x)
end proc:

convergenceRadius := proc(a::{procedure})
  local an, lim;

  an := abs(a(n+1)/a(n)):
  print('abs(a(n+1)/a(n))' = simplify(an));

  lim := limit(an,n=infinity):
  print('limit(abs(a(n+1)/a(n)),n=infinity)' = lim);
  print(lim &lt; 1);

  solve(
    limit(abs(a(n+1)/a(n)),n=infinity) &lt; 1
  );
end proc;

dtransform := proc(eq)
local leq, n, c, j, i, test_eq, coeff_row, A, b, d;
leq := lhs(eq):

n := PDEtools[difforder](leq):
c := PDEtools[dcoeffs](leq, y(t)):


coeff_row := []:
test_eq := leq - c[1]*diff(y(t),t$n):

j := 2:

for i from n-1 by -1 to 1 do
if(has(test_eq,diff(y(t),t$i))) then
    coeff_row := [op(coeff_row),-c[j]/c[1]]:
    test_eq := test_eq - c[j]*diff(y(t),t$i):
    j += 1:
else
coeff_row := [op(coeff_row),0]:

end if;
end do;
if(has(test_eq,y(t))) then
    coeff_row := [op(coeff_row),-c[j]/c[1]]:
end if;

A := &lt;&lt;&lt;seq(0,i=1..n-1)&gt;|LinearAlgebra[IdentityMatrix](n-1)&gt;;Matrix(coeff_row)&gt;:
b := &lt;seq(0,i=1..n-1),1/c[1]&gt;;
d := &lt;1,seq(0,i=1..n-1)&gt;;
return A, b, d;
end proc;

end module;</Text-field>
